    </style>
    <style name="CustomDialog" parent="dialog_style">
        <!-- 出现动画 -->
        <item name="android:windowAnimationStyle">@style/PopupAnimation</item>
        <!-- 模糊 背景变暗 -->
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="PopupAnimation" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/popup_alpha_in</item>
        <item name="android:windowExitAnimation">@anim/popup_alpha_out</item>
    </style>
    <style name="dialog_style" parent="android:Theme.Dialog">
        <!-- 边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- 半透明 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 无标题 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 背景透明 -->
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
